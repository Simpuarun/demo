package TicketBook;

public class CharCheck {

	public static void main(String[] args) {
		
		
		String input = "saravanansssss";
		char search[] = {'a','s'};
		
		int count= 0;
		
		for (int i = 0; i<=input.length(); i++)
		{
			if(input.charAt(i) == search[i] )
				count++;
			
		}
	System.out.println("The Character appears "+count+" times." );
	
	}
	

}
